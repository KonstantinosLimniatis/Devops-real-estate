[Unit]
Description=spring boot daemon
After=network.target

[Service]
User={{appuser}}
Group={{appgroup}}
WorkingDirectory={{service.workingdir}}
{% for item in app.env | dict2items %}
Environment="{{ item.key | upper | replace('.', '_') }}={{ item.value }}"
{% endfor %}
ExecStart={{service.execstart}}

[Install]
WantedBy=multi-user.target
